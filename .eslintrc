{
	"extends": ["airbnb", "prettier", "prettier/react"],
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [".js", ".jsx"]
			}
		],
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "none",
				"singleQuote": false,
				"printWidth": 80
			}
		]
	},
	"plugins": ["prettier"]
}
